public class CarComparison {
    public static void main(String[] args) {
        Car car1 = new Car("Gas Car");
        Car car2 = new Car("Gas Car");
        ElectricCar electricCar = new ElectricCar("Electric Car");

        System.out.println("Are car1 and car2 of the same type? " + compareObjects(car1, car2));
        System.out.println("Are car1 and electricCar of the same type? " + compareObjects(car1, electricCar));
    }

    static class Car {
        private String model;

        public Car(String model) {
            this.model = model;
        }

        public String getModel() {
            return model;
        }
    }

    static class ElectricCar extends Car {
        public ElectricCar(String model) {
            super(model);
        }
    }

    public static <T extends Car> boolean compareObjects(T object1, T object2) {
        return object1.getClass().equals(object2.getClass());
    }
}
