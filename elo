
public class Dog {
    private String name;
    private String breed;
    private int age;

    public Dog(String name, String breed, int age) {
        this.name = name;
        this.breed = breed;
        this.age = age;
    }

    public void bark() {
        System.out.println("Hau Hau");
    }

    public static void main(String[] args) {
        Dog myDog = new Dog("Burek", "Golden Retriever", 3);
        myDog.bark();
    }
}
