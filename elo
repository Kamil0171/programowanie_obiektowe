public class Dog {
    String name;
    String breed;
    int age;

    public void bark() {
        System.out.println("Hau Hau");
    }

    public static void main(String[] args) {
        Dog myDog = new Dog();
        myDog.name = "Buddy";
        myDog.breed = "Labrador";
        myDog.age = 3;

        myDog.bark(); // Wywo≈Çanie metody bark() na obiekcie myDog
    }
}
